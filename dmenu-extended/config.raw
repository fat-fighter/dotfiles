{
	"alias_applications": false,
	"alias_display_format": "{name} ({command})",
	"exclude_items": [],
	"filebrowser": "xdg-open",
	"fileopener": "xdg-open",
	"filter_binaries": false,
	"follow_symlinks": false,
	"frequently_used": 0,
	"global_ignore_folders": [],
	"ignore_folders": [],
	"include_applications": true,
	"include_binaries": false,
	"include_hidden_files": false,
	"include_hidden_folders": false,
	"include_items": [
		[
			"View Running Processes",
			"htop;"
		]
	],
	"indicator_alias": "#",
	"indicator_edit": "*",
	"indicator_submenu": ">",
	"menu": "rofi",
	"menu_arguments": [
		"-dmenu",
		"-theme",
		"fatmenu",
		"-i"
	],
	"password_helper": [
		"zenity",
		"--password",
		"--title={prompt}"
	],
	"path_aliasFile": "/home/fat-fighter/.zsh_aliases",
	"path_shellCommand": "~/.dmenuEextended_shellCommand.sh",
	"prompt": "Run",
	"scan_hidden_folders": false,
	"terminal": "terminology",
	"valid_extensions": [
		"sh",
		"py",
		"svg",
		"pdf",
		"txt",
		"png",
		"jpg",
		"gif",
		"php",
		"tex",
		"odf",
		"ods",
		"avi",
		"mpg",
		"mp3",
		"lyx",
		"bib",
		"iso",
		"ps",
		"zip",
		"xcf",
		"doc",
		"docxxls",
		"xlsx",
		"md",
		"html",
		"sublime-project"
	],
	"watch_folders": [
		"~/{{iitk-path}}",
		"~/{{books-path}}",
		"~/{{pictures-path}}",
		"~/{{projects-path}}"
	],
	"webbrowser": "xdg-open"
}
